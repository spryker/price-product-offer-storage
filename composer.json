{
  "name": "spryker/price-product-offer-storage",
  "type": "library",
  "description": "PriceProductOfferStorage module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/currency": "^3.1.0",
    "spryker/event": "^2.0.0",
    "spryker/event-behavior": "^1.14.0",
    "spryker/kernel": "^3.30.0",
    "spryker/price-product": "^4.9.0",
    "spryker/price-product-extension": "^1.2.0",
    "spryker/price-product-offer": "^0.5.0",
    "spryker/price-product-offer-storage-extension": "^0.1.0",
    "spryker/price-product-storage": "^4.4.0",
    "spryker/price-product-storage-extension": "^1.2.0",
    "spryker/product": "^6.0.0",
    "spryker/product-offer": "^0.2.0 || ^0.3.0",
    "spryker/propel-orm": "^1.5.0",
    "spryker/storage": "^3.4.0",
    "spryker/store": "^1.10.0",
    "spryker/synchronization": "^1.0.0",
    "spryker/synchronization-behavior": "^1.0.0",
    "spryker/synchronization-extension": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/merchant": "*",
    "spryker/propel": "*",
    "spryker/queue": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
